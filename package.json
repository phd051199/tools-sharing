{
  "name": "tools-sharing-vuetify",
  "version": "0.0.0",
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "dev": "vite",
    "format": "prettier --write .",
    "postinstall": "simple-git-hooks",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "preview": "vite preview"
  },
  "simple-git-hooks": {
    "pre-commit": "bun lint-staged --allow-empty --concurrent false",
    "commit-msg": "bun commitlint -e $1"
  },
  "lint-staged": {
    "*.${vue,ts,tsx,js,jsx,less,css}": "bun format",
    "*.{vue,ts,tsx,js,jsx}": "bun lint"
  },
  "dependencies": {
    "@highlightjs/vue-plugin": "^2.1.0",
    "@mdi/font": "6.2.95",
    "@tanstack/vue-query": "^5.45.0",
    "axios": "^1.7.2",
    "core-js": "^3.37.1",
    "dayjs": "^1.11.11",
    "idb": "^8.0.0",
    "lodash": "^4.17.21",
    "vue": "^3.4.29",
    "vue3-google-signin": "^1.3.5",
    "vuetify": "^3.6.10"
  },
  "devDependencies": {
    "@babel/types": "^7.24.7",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@iconify-json/material-symbols": "^1.1.82",
    "@types/lodash": "^4.17.5",
    "@types/node": "^20.14.7",
    "@unocss/eslint-config": "^0.61.0",
    "@vite-pwa/assets-generator": "^0.2.4",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-typescript": "^13.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "eslint-plugin-vue": "^9.26.0",
    "lint-staged": "^15.2.7",
    "pinia": "^2.1.7",
    "prettier": "^3.3.2",
    "sass": "^1.77.6",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.5.2",
    "unocss": "^0.61.0",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-fonts": "^1.1.1",
    "unplugin-icons": "^0.19.0",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-router": "^0.8.8",
    "vite": "^5.3.1",
    "vite-plugin-pwa": "^0.20.0",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vite-plugin-vuetify": "^2.0.3",
    "vue-router": "^4.3.3",
    "vue-tsc": "^2.0.21"
  }
}
